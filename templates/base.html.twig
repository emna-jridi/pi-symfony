<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <!-- plugins:css -->
    {% block stylesheets %}
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
        <link rel="stylesheet" href="{{ asset('FrontOffice/vendors/feather/feather.css') }}">
        <link rel="stylesheet" href="{{ asset('FrontOffice/vendors/ti-icons/css/themify-icons.css') }}">
        <link rel="stylesheet" href="{{ asset('FrontOffice/vendors/css/vendor.bundle.base.css') }}">
        <link rel="stylesheet" href="{{ asset('FrontOffice/vendors/datatables.net-bs4/dataTables.bootstrap4.css') }}">
        <link rel="stylesheet" href="{{ asset('FrontOffice/vendors/ti-icons/css/themify-icons.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('FrontOffice/js/select.dataTables.min.css') }}">
        <!-- inject:css -->
        <link rel="stylesheet" href="{{ asset('FrontOffice/css/vertical-layout-light/style.css') }}">
        <!-- endinject -->
        <link rel="shortcut icon" href="{{ asset('FrontOffice/images/favicon.png') }}">
    {% endblock %}
</head>
<body>
<div class="container-scroller">
    <!-- partial:partials/_navbar.html -->
    {% include 'partials/_navbar.html.twig' %}

    <!-- partial -->
    <div class="container-fluid page-body-wrapper">
        <!-- partial:partials/_sidebar.html -->
        {% include 'partials/_sidebar.html.twig' %}

        <!-- partial -->
        <div class="main-panel">
            <div class="content-wrapper">
                {% block body %}
                    {# Default dashboard content for pages that don't override the body block #}




                {% endblock %}
            </div>
            <!-- content-wrapper ends -->
            <!-- partial:partials/_footer.html -->
            {% include 'partials/_footer.html.twig' %}
            <!-- partial -->
        </div>
        <!-- main-panel ends -->
    </div>
    <!-- page-body-wrapper ends -->
</div>
<!-- container-scroller -->

<!-- plugins:js -->
{% block javascripts %}
    <script src="{{ asset('FrontOffice/vendors/js/vendor.bundle.base.js') }}"></script>
    <!-- endinject -->
    <!-- Plugin js for this page -->
    <script src="{{ asset('FrontOffice/vendors/chart.js/Chart.min.js') }}"></script>
    <script src="{{ asset('FrontOffice/vendors/datatables.net/jquery.dataTables.js') }}"></script>
    <script src="{{ asset('FrontOffice/vendors/datatables.net-bs4/dataTables.bootstrap4.js') }}"></script>
    <script src="{{ asset('FrontOffice/js/dataTables.select.min.js') }}"></script>
    <!-- End plugin js for this page -->
    <!-- inject:js -->
    <script src="{{ asset('FrontOffice/js/off-canvas.js') }}"></script>
    <script src="{{ asset('FrontOffice/js/hoverable-collapse.js') }}"></script>
    <script src="{{ asset('FrontOffice/js/template.js') }}"></script>
    <script src="{{ asset('FrontOffice/js/settings.js') }}"></script>
    <script src="{{ asset('FrontOffice/js/todolist.js') }}"></script>
    <!-- endinject -->
    <!-- Custom js for this page -->
    <script src="{{ asset('FrontOffice/js/dashboard.js') }}"></script>
    <script src="{{ asset('FrontOffice/js/Chart.roundedBarCharts.js') }}"></script>
    <!-- End custom js for this page -->
{% endblock %}
</body>
</html>